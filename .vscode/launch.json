  "version": "0.2.0",
  "configurations": [
    {
        "name": "Start typescript-test",
		"runtimeVersion": "20.9.0",
		"type": "node",
		"request": "launch",
		"cwd": "${workspaceFolder}",
		"runtimeExecutable": "npm",
		"runtimeArgs": ["run-script", "start"],
		"stopOnEntry": true,
		"sourceMaps": true,
		"autoAttachChildProcesses": true,
		"showAsyncStacks": true,
		"smartStep": true
    },
	{
		"name": "Start Index",
		"runtimeVersion": "20.9.0",
		"type": "node",
		"request": "launch",
		"cwd": "${workspaceFolder}",
		"runtimeExecutable": "npm",
		"runtimeArgs": ["run-script", "start:index"],
		"stopOnEntry": true,
		"sourceMaps": true,
		"autoAttachChildProcesses": true,
		"showAsyncStacks": true,
		"smartStep": true
	  }
  ]
}
